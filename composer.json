{
    "name": "siciarek/photogallery-bundle",
    "type": "symfony-bundle",
    "description": "Photo Gallery Bundle for use with Symfony2",
    "keywords": [
        "photo",
        "gallery",
        "images",
        "bundle"
    ],
    "homepage": "http://github.com/siciarek/photogallery-bundle.git",
    "license": "MIT",
    "authors": [
        {
            "name": "Jacek Siciarek",
            "email": "siciarek@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.2",
        "symfony/symfony": "2.1.*",
        "imagine/Imagine": "v0.3.0",
        "stof/doctrine-extensions-bundle": "dev-master",
        "gedmo/doctrine-extensions": "dev-master",
        "stfalcon/tinymce-bundle": "dev-master",
        "friendsofsymfony/user-bundle": "*",
        "friendsofsymfony/jsrouting-bundle": "1.0.*"
    },
    "require-dev": {
        "symfony/framework-bundle": ">=2.1,<2.2-dev"
    },
    "autoload": {
        "psr-0": {
            "Siciarek\\PhotoGalleryBundle": ""
        }
    },
    "minimum-stability": "dev",
    "target-dir": "Siciarek/PhotoGalleryBundle"
}
